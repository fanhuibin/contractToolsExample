{
  "version": "1.0",
  "contractType": "construction",
  "defaults": {"type": "string", "required": false, "normalize": ["trim"]},
  "fields": {
    "合同编号": {"type": "string", "normalize": ["trim"]},
    "合同名称": {"type": "string", "required": true, "normalize": ["trim", {"regexReplace": ["(合同|合同书)$", ""]}], "denyContains": ["合同", "合同书"], "maxLength": 100, "promptHints": ["返回纯名称，不包含‘合同/合同书’字样"]},
    "甲方名称": {"type": "string", "normalize": ["trim"], "maxLength": 200},
    "乙方名称": {"type": "string", "normalize": ["trim"], "maxLength": 200},
    "甲方联系地址": {"type": "string", "normalize": ["trim"]},
    "乙方联系地址": {"type": "string", "normalize": ["trim"]},
    "甲方联系人及电话": {"type": "string", "normalize": ["trim"]},
    "乙方联系人及电话": {"type": "string", "normalize": ["trim"]},
    "合同签署日期": {"type": "date", "normalize": [{"parseDate": ["yyyy-MM-dd", "yyyy/MM/dd", "yyyy年MM月dd日", "yyyy.MM.dd"]}], "outputFormat": "yyyy-MM-dd"},
    "合同有效期": {"type": "string", "normalize": ["trim"]},
    "合同起始时间": {"type": "date", "normalize": [{"parseDate": ["yyyy-MM-dd", "yyyy/MM/dd", "yyyy年MM月dd日", "yyyy.MM.dd"]}], "outputFormat": "yyyy-MM-dd"},
    "合同终止时间": {"type": "date", "normalize": [{"parseDate": ["yyyy-MM-dd", "yyyy/MM/dd", "yyyy年MM月dd日", "yyyy.MM.dd"]}], "outputFormat": "yyyy-MM-dd"},
    "签署人信息": {"type": "string", "normalize": ["trim"]},
    "违约责任条款": {"type": "string", "normalize": ["trim"]},
    "争议解决方式": {"type": "string", "normalize": ["trim"]},
    "适用法律": {"type": "string", "normalize": ["trim"]},
    "工程名称": {"type": "string"},
    "工程地点": {"type": "string"},
    "工程内容": {"type": "string"},
    "工程范围": {"type": "string"},
    "承包方式": {"type": "string"},
    "合同价款": {"type": "number", "normalize": ["trim", {"regexKeep": "[\\d\\.\\-]+"}], "min": 0},
    "付款进度安排": {"type": "string"},
    "工程期限": {"type": "string"},
    "开工日期": {"type": "date", "normalize": [{"parseDate": ["yyyy-MM-dd", "yyyy/MM/dd", "yyyy年MM月dd日", "yyyy.MM.dd"]}], "outputFormat": "yyyy-MM-dd"},
    "竣工日期": {"type": "date", "normalize": [{"parseDate": ["yyyy-MM-dd", "yyyy/MM/dd", "yyyy年MM月dd日", "yyyy.MM.dd"]}], "outputFormat": "yyyy-MM-dd"},
    "质量标准": {"type": "string"},
    "工程变更条款": {"type": "string"},
    "工程验收标准": {"type": "string"},
    "质保金比例": {"type": "number", "range": [0, 100]},
    "质保期限": {"type": "string"},
    "安全责任条款": {"type": "string"},
    "工程进度报告要求": {"type": "string"},
    "材料供应方式": {"type": "string"},
    "违约责任": {"type": "string"}
  }
}

